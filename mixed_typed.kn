// Test mixed typed and untyped functions
// This will help identify if the issue is with mixing typed and untyped functions

// Function without type annotations
add_simple(x, y) = x + y

// Function with type annotations
add_typed(x: Int, y: Int): Int = x + y

// Function with only return type
multiply_return_typed(a, b): Int = a * b

// Function with only parameter types
divide_param_typed(x: Int, y: Int) = x / y

// Main function to test all of them
main() = add_simple(10, 20)
