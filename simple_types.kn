// Minimal test for type annotations on function parameters
// Testing why type annotations cause parsing failures

// Simple function without types (should work)
add_simple(x, y) = x + y

// Function with type annotations (currently fails)
add_typed(x: Int, y: Int) = x + y

// Function with return type annotation
add_with_return(x, y): Int = x + y

// Function with both parameter and return types
add_full_typed(x: Int, y: Int): Int = x + y

// Main function
main() = add_simple(10, 20)
